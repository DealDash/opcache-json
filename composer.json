{
  "name": "stevencorona/opcache-json",
  "description": "Library that grabs some critical stats from Zend Opcache and outputs to JSON for monitoring and alerting. Can also output to statsd sink.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Steve Corona",
      "email": "hello@scalingphpbook.com",
      "homepage": "http://scalingphpbook.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-json": "*",
    "domnikl/statsd": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.3.*"
  },
  "autoload": {
    "psr-4": {"Opcache\\": "src/Opcache"}
  }
}
